import matplotlib.pyplot as plt
n=100
a,b=-1,1
incremento=(b-a)/(n-1)

vx=[a+incremento*i for i in range(n)]
vy=[i*i for i in vx]

plt.xlabel("eje a")
plt.ylabel("el otro eje")
plt.title("ejemplo")
plt.plot(vx,vy)
