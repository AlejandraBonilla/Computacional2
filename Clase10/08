# que tengo la siguiente operaci√≥n n!=n(n-1)!
def fac(n):
    if n==0: return 1
    else: return  n*fac(n-1)

print (fac(5))

print ("*"*30)
#para obtener el factorial de 1 hasta 5 
#donde n!=n(n-1)...1
fac=1 
n=6

for i in range(1,6):
    fac=i*fac
    print (fac)



